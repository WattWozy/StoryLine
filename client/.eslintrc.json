{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "next/core-web-vitals",
    "airbnb"
  ],
  "plugins" :[
    "react"
  ],
  "rules": {
    "camelcase":[2, {"ignoreImports": true}],
    "react/prop-types": [0],
    "react/function-component-definition": [2, {
        "namedComponents": "arrow-function",
        "unnamedComponents": "arrow-function"
      }]
  }
}